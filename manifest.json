
{
  "manifest_version": 3,
  "name": "Site Time Tracker",
  "version": "1.0.0",
  "description": "Track and analyze your daily website usage patterns",
  "permissions": [
    "storage",
    "tabs",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
